package chp5;

public class Cube {
	
	private int num1 = 0;
	
	public Cube ()
	
	{
		
	this.num1 = (int) Math.pow (num1, 3);
	
	}
	
	
	public int getnum1()
	
	{
		
		return num1;
		
	}	
	
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("That value raised to the third power is: " + num1);
		return builder.toString();
	}
	
}